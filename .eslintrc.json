{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "Transifex": true,
    "google": true
  },
  "extends": "vizzuality",
  "rules": {
    "import/prefer-default-export": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "parser": "babel-eslint",
  "plugins": [
    "babel"
  ],
  "settings": {
    "import/resolver": {
      "babel-module": {
      }
    }
  }
}
